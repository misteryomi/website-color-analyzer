{
    "manifest_version": 3,
    "name": "Website Color Usage Analyzer",
    "version": "1.1.0",
    "description": "Analyze, review usage and copy colors used on websites you love!",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}